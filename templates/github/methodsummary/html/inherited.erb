<div class="inherited">
  <% inherited_meths_by_class do |klass, meths| %>
  <div>
    <h2><%= visibility.to_s.capitalize %> <%= scope.to_s.capitalize %> Methods Inherited from <%= linkify klass %></h2>
    <p><%= format_object_name_list meths %></p>
  </div>
  <% end %>
</div>
<div class="clear"></div>